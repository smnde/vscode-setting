{
	"workbench.colorTheme": "Andromeda Bordered",
	"workbench.iconTheme": "vscode-icons",
	"editor.fontLigatures": true,
	"editor.fontFamily": "'Fira Code', 'Source Code Pro',  'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"diffEditor.wordWrap": "on",
	"editor.minimap.enabled": false,
	"editor.wordWrap": "on",
	"editor.accessibilitySupport": "off",
	"[javascript,]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": true
	},
	"css.validate": false,
	"stylelint.enable": true,
	"scss.validate": false,
	"tabnine.experimentalAutoImports": true,
	"intelephense.diagnostics.enable": false,
	"intelephense.diagnostics.typeErrors": false,
	"intelephense.diagnostics.undefinedSymbols": false,
	"intelephense.diagnostics.undefinedVariables": false,
	"bracketPairColorizer.depreciation-notice": false,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"prettier.useTabs": true,
	"editor.insertSpaces": false,
	"explorer.confirmDelete": false,
	"liveServer.settings.donotVerifyTags": true,
	"arduino.enableUSBDetection": true,
	"workbench.editor.splitInGroupLayout": "vertical",
	"window.zoomLevel": 1,
	"files.autoSave": "afterDelay",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"editor.fontWeight": "normal",
	"prettier.bracketSpacing": false,
	"prettier.enable": false,
	"files.autoGuessEncoding": true,
	"editor.stickyScroll.enabled": true,
	"editor.formatOnPaste": true,
	"settingsSync.ignoredExtensions": [
	
	]
}
